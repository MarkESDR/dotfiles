# Ubuntu 18 mouse fix on sleep
alias scroll_fix='sudo modprobe -r psmouse && sudo modprobe psmouse'

alias "cd.."="cd .."
alias h="history"

alias gaa="git add ."
alias gcm="git commit -m"
alias gaacm="git add . && git commit -m"
alias gltime="git log --pretty=format:\"%h%x09%an%x09%ad%x09%s\""

alias capsctrl="setxkbmap -option ctrl:nocaps"

alias nv=nvim
alias nvi=nvim
